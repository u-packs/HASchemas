{
	"$id": "https://raw.githubusercontent.com/u-packs/HASchemas/main/schemas/websocket/request/ping.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"description": "https://developers.home-assistant.io/docs/api/websocket/#pings-and-pongs",
	"type": "object",
	"title": "PingMsg",
	"properties": {
		"type": {
			"description": "Type key of the message.",
			"type": "string",
			"const": "ping",
			"default": "ping"
		},
		"id": {
			"description": "The ID of the response will point at the original ID of this message.",
			"type": "integer",
			"minimum": 0
		}
	},
	"required": [
		"id",
		"type"
	],
	"additionalProperties": false
}
