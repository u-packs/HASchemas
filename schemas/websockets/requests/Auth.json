{
	"$id": "https://raw.githubusercontent.com/u-packs/HASchemas/main/schemas/websockets/requests/Auth.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"description": "https://developers.home-assistant.io/docs/api/websocket/#authentication-phase",
	"type": "object",
	"title": "AuthMsg",
	"properties": {
		"type": {
			"description": "Type key of the message.",
			"type": "string",
			"const": "auth",
			"default": "auth"
		},
		"access_token": {
			"description": "The access token for connection authorization.",
			"type": "string"
		}
	},
	"required": [
		"access_token",
		"type"
	],
	"additionalProperties": false
}
