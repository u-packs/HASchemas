{
	"id": "https://raw.githubusercontent.com/u-packs/HASchemas/main/schemas/websockets/requests/Auth.json",
	"$schema": "http://json-schema.org/draft-06/schema#",
	"description": "https://developers.home-assistant.io/docs/api/websocket/#authentication-phase",
	"type": "object",
	"title": "Auth",
	"properties": {
		"access_token": {
			"description": "The access token for this connection.",
			"type": "string"
		},
		"type": {
			"type": "string",
			"default": "auth"
		}
	},
	"required": [
		"access_token",
		"type"
	],
	"additionalProperties": false
}
